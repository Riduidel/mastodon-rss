{
    "name": "riduidel/mastodon-rss",
    "description": "This small project reads the timeline of a Mastodon user into an RSS feed, allowing me to browse mastodon in the comfort of my RSS reader of choice",
    "type": "project",
    "license": "GPL",
    "config": {
        "platform": {
            "php": "7.4.999"
        }
    },
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Nicolas Delsaux",
            "email": "nicolas.delsaux@gmx.fr"
        }
    ],
    "require": {
        "phediverse/mastodon-rest": "dev-master",
        "chdemko/sorted-collections": "1.0.7"
    },
    "archive": {
        "exclude": [
            ".*",
            "!*.php",
            "cache/",
            "node_modules/",
            "*.zip"
        ]
    }
}
